package com.io;

public class Employee {
 private int eno;
 private String name;
 private String city;
 
 
public Employee() {
	super();
	// TODO Auto-generated constructor stub
}

public Employee(int eno, String name, String city) {
	super();
	this.eno = eno;
	this.name = name;
	this.city = city;
}

public int getEno() {
	return eno;
}
public void setEno(int eno) {
	this.eno = eno;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public String getCity() {
	return city;
}
public void setCity(String city) {
	this.city = city;
}
  
  
}
