package com.io;

import java.io.Serializable;

public class Employee implements Serializable {
 private int eno;
 private String name;
 private String city;
 private transient int age;
 
 
public Employee() {
	super();
	// TODO Auto-generated constructor stub
}

public Employee(int eno, String name, String city,int age) {
	super();
	this.eno = eno;
	this.name = name;
	this.city = city;
	this.age=age;
}

public int getEno() {
	return eno;
}
public void setEno(int eno) {
	this.eno = eno;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public String getCity() {
	return city;
}
public void setCity(String city) {
	this.city = city;
}

@Override
public String toString() {
	return "Employee [eno=" + eno + ", name=" + name + ", city=" + city + "]";
}
  
  
}
