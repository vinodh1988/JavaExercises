  public static void main(String n[]){
	  File f=new File("d:\\CSS3.png");
	   System.out.println( f.length());
	  FileInputStream f1;
	  try {
		f1=new FileInputStream(f);
		System.out.println();
	} catch (FileNotFoundException e) {
