package com.util;

import java.util.HashMap;
import java.util.Map;

import com.io.Employee;

public class MapDemo {
    public static void main(String n[]){
    	Map p=new HashMap();
        p.put("Raj", new Employee(1,"Raj","Chennai",48));
        p.put("Ram",new Employee(2,"Ram","Chennai",24));
        p.put("Raj", new Employee(3,"Raj","Nagpur",34));
    }
}
